                    TextLabel.Parent = TExtBox
                    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextLabel.BackgroundTransparency = 1.000
                    TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    TextLabel.BorderSizePixel = 0
                    TextLabel.Position = UDim2.new(0.0315789469, 0, 0.151515156, 0)
                    TextLabel.Size = UDim2.new(0, 75, 0, 22)
                    TextLabel.Font = Enum.Font.SourceSansBold
                    TextLabel.Text = boxname
                    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                    TextLabel.TextSize = 14.000

                    TextBox.Parent = TExtBox
                    TextBox.BackgroundColor3 = Color3.fromRGB(77, 77, 77)
                    TextBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    TextBox.BorderSizePixel = 0
                    TextBox.Position = UDim2.new(0.677839756, 0, 0.121212125, 0)
                    TextBox.Size = UDim2.new(0, 43, 0, 25)
                    TextBox.Font = Enum.Font.SourceSans
                    TextBox.Text = ""
                    TextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
                    TextBox.TextSize = 14.000

                    UICorner_8.Parent = TextBox

                    TextBox.FocusLost:Connect(
                        function()
                            Callbaclk(TextBox.Text)
                        end
                    )
                end
            return elements
        end
    return tabs
end
