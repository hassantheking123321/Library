    outline.BorderSizePixel = 1

                    local SLIDERNAME = Instance.new("TextLabel")
    local outline = Instance.new("TextLabel")
    outline.BorderColor3 = Color3.new(1, 1, 1) -- White color
    outline.BorderSizePixel = 1

                    local CALC = Instance.new("TextLabel")
    local outline = Instance.new("TextLabel")
